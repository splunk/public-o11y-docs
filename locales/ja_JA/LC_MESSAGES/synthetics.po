msgid "First paint time"
msgstr "最初の描画時間"

#: ../../synthetics/browser-test/browser-test-metrics.rst:127
msgid "``synthetics.first_paint.time.ms``"
msgstr "``synthetics.first_paint.time.ms``"

#: ../../synthetics/browser-test/browser-test-metrics.rst:128
